{
  "expo": {
    "name": "श्री जी",
    "slug": "shree-jii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shreejii.app",
      "infoPlist": {
        "NSMotionUsageDescription": "This app uses motion sensors to track your daily steps."
      }
    },
    "android": {
      "package": "com.shreejii.app",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACTIVITY_RECOGNITION",
        "BODY_SENSORS",
        "ACTIVITY_RECOGNITION"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundImage": "./assets/images/app-icon.png",
        "monochromeImage": "./assets/images/app-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "notification": {
      "icon": "./assets/images/app-icon.png",
      "color": "#FFD700"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sensors",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app-icon.png",
          "color": "#FFD700",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shree-ji-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "673fabf5-03eb-4cf6-ae5f-291bcbc5fde3"
      }
    }
  }
}